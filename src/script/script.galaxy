bool gt_help_Func (bool testConds, bool runActions) {
    // Conditions
    if (testConds) {
        if (!(((StringCompare(StringWord(EventChatMessage(false), 1), "-help", c_stringCase) == 0)))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    UIDisplayMessage(PlayerGroupSingle(EventPlayer()), c_messageAreaChat, StringExternal("Param/Value/E70BD3A7"));
    UIDisplayMessage(PlayerGroupSingle(EventPlayer()), c_messageAreaChat, StringExternal("Param/Value/262A47CA"));
    UIDisplayMessage(PlayerGroupSingle(EventPlayer()), c_messageAreaChat, StringExternal("Param/Value/AB656648"));
    UIDisplayMessage(PlayerGroupSingle(EventPlayer()), c_messageAreaChat, StringExternal("Param/Value/D272B308"));
    return true;
}

