// Trigger: players
//--------------------------------------------------------------------------------------------------
bool gt_players_Func (bool testConds, bool runActions) {
    // Conditions
    if (testConds) {
        if (!(((StringCompare(StringWord(EventChatMessage(false), 1), "-players", c_stringCase) == 0)))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    PlayerGroupLoopBegin(PlayerGroupActive());
    for ( ; !PlayerGroupLoopDone() ; PlayerGroupLoopStep() ) {
        UIDisplayMessage(PlayerGroupSingle(EventPlayer()), c_messageAreaChat, (StringExternal("Param/Value/BC4736E3") + IntToText(PlayerGroupLoopCurrent()) + StringExternal("Param/Value/B794884B") + PlayerName(PlayerGroupLoopCurrent()) + StringExternal("Param/Value/57032F77")));
    }
    PlayerGroupLoopEnd();
    return true;
}

