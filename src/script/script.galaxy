    gt_player = TriggerCreate("gt_player_Func");
    TriggerAddEventChatMessage(gt_player, c_playerAny, "-player", false);
}

//--------------------------------------------------------------------------------------------------
// Trigger: kick
//--------------------------------------------------------------------------------------------------
bool gt_kick_Func (bool testConds, bool runActions) {
    // Variable Declarations
    int lv_sum;

    // Variable Initialization

    // Conditions
    if (testConds) {
        if (!(((gv_votelock == false) && (StringCompare(StringWord(EventChatMessage(false), 1), "-kick", c_stringCase) == 0) && ((StringToInt(StringWord(EventChatMessage(false), 2)) >= 1) && (StringToInt(StringWord(EventChatMessage(false), 2)) <= 8)) && (EventPlayer() == StringToInt(StringWord(EventChatMessage(false), 2))) && (PlayerGroupHasPlayer(PlayerGroupAlliance(c_playerGroupAlly, EventPlayer()), StringToInt(StringWord(EventChatMessage(false), 2))) == true)))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    gv_votelock = true;
    UIDisplayMessage(PlayerGroupAll(), c_messageAreaChat, (StringExternal("Param/Value/4875D7E8") + PlayerName(EventPlayer()) + StringExternal("Param/Value/B2D2B54B") + PlayerName(StringToInt(StringWord(EventChatMessage(false), 2))) + StringExternal("Param/Value/DA8BCF1E")));
    IntLoopBegin(1, 10);
    for ( ; !IntLoopDone() ; IntLoopStep() ) {
        gv_votes[IntLoopCurrent()] = 0;
    }
    IntLoopEnd();
    TriggerEnable(gt_yes, true);
    TriggerEnable(gt_no, true);
    Wait(15.0, c_timeReal);
    TriggerEnable(gt_yes, false);
    TriggerEnable(gt_no, false);
    IntLoopBegin(1, 10);
    for ( ; !IntLoopDone() ; IntLoopStep() ) {
        lv_sum += gv_votes[IntLoopCurrent()];
    }
    IntLoopEnd();
    if ((lv_sum > 0)) {
        UIDisplayMessage(PlayerGroupAll(), c_messageAreaChat, StringExternal("Param/Value/AE420E12"));
        KickFromGame(PlayerGroupSingle(StringToInt(StringWord(EventChatMessage(false), 2))));
    }
    else {
        UIDisplayMessage(PlayerGroupAll(), c_messageAreaChat, StringExternal("Param/Value/9330F4C4"));
    }
    gv_votelock = false;
    return true;
}

//--------------------------------------------------------------------------------------------------
void gt_kick_Init () {
    gt_kick = TriggerCreate("gt_kick_Func");
    TriggerAddEventChatMessage(gt_kick, c_playerAny, "-kick", false);
}

