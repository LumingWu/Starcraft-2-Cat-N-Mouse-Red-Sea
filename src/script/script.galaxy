//--------------------------------------------------------------------------------------------------
bool gt_player_Func (bool testConds, bool runActions) {
    // Conditions
    if (testConds) {
        if (!(((StringCompare(StringWord(EventChatMessage(false), 1), "-player", c_stringCase) == 0) && ((StringToInt(StringWord(EventChatMessage(false), 2)) >= 1) && (StringToInt(StringWord(EventChatMessage(false), 2)) <= 8))))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    UIDisplayMessage(PlayerGroupSingle(EventPlayer()), c_messageAreaChat, (StringExternal("Param/Value/C5B87BBF") + IntToText(StringToInt(StringWord(EventChatMessage(false), 2))) + StringExternal("Param/Value/7B54AF5A") + PlayerName(StringToInt(StringWord(EventChatMessage(false), 2))) + StringExternal("Param/Value/2E0B71EE")));
    return true;
}

